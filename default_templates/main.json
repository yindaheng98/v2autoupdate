{
  "inbounds": [
    {
      "port": 1080,
      "listen": "0.0.0.0",
      "protocol": "http",
      "settings": {
        "udp": true
      }
    }
  ],
  "outbounds": [
    ${OUTBOUNDS}
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {}
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "balancerTag": "default-balancer",
        "domain": [
          "geosite:tld-!cn",
          "geosite:geolocation-!cn"
        ]
      },
      {
        "type": "field",
        "outboundTag": "direct",
        "domain": [
          "geosite:private",
          "geosite:cn"
        ],
        "ip": [
          "geoip:cn"
        ]
      }
    ],
    "balancers": [
      {
        "tag": "default-balancer",
        "selector": [
          ${TAGS}
          "default-proxy"
        ],
        "strategy": {
          "type": "random"
        }
      }
    ]
  }
}
