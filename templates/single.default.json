{
    "inbounds": [
      {
        "port": 1080,
        "listen": "0.0.0.0",
        "protocol": "socks",
        "sniffing": {
          "enabled": true,
          "destOverride": [
            "http",
            "tls"
          ]
        },
        "settings": {
          "auth": "noauth",
          "udp": true
        }
      }
    ],
    "outbounds": [
      ${OUTBOUND}
      {
        "tag": "direct",
        "protocol": "freedom",
        "settings": {}
      }
    ],
    "routing": {
      "domainStrategy": "IPIfNonMatch",
      "rules": [
        {
          "type": "field",
          "outboundTag": "${SELECTOR}",
          "domain": [
            "!geosite:cn"
          ]
        },
        {
          "type": "field",
          "outboundTag": "direct",
          "domain": [
            "geosite:private",
            "geosite:cn"
          ],
          "ip": [
            "geoip:cn"
          ]
        }
      ]
    }
  }
  